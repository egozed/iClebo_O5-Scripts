#!/bin/sh
cd / && echo "cd /"
/mnt/nand/root/kill.sh && echo "kill main process"
/bin/umount /mnt/dsk3 && echo "unmount readonly wiifi_config_dsk"
/bin/mount -t vfat /dev/mtdblock8 /mnt/dsk3 && echo "mount read-write wiifi_config_dsk"
/bin/rm -rf /mnt/dsk3/* && echo "delete all from wiifi_config dsk"
/bin/cp -r /mnt/dsk1/etc /mnt/dsk3/ && echo "copy from factory_config_dsk/etc/ to wiifi_config_dsk/"
/bin/cp -r /mnt/dsk1/root /mnt/dsk3/ && echo "copy from factory_config_dsk/root/ to wiifi_config_dsk/"
/bin/umount /mnt/dsk3 && echo "unmount rw wiifi_config_dsk"
/bin/mount -t vfat -r /dev/mtdblock8 /mnt/dsk3 && echo "mount readonly wiifi_config_dsk"
/bin/rm -rf /mnt/dsk5/* && echo "delete all from map_dsk"
/bin/rm -rf /mnt/dsk2/* && echo "delete all from node_config_dsk"
/bin/cp -r /mnt/dsk1/home /mnt/dsk2/ && echo "copy from factory_config_dsk/home/ to node_config_dsk/"
/bin/rm -rf /mnt/dsk2/map && echo "delete from node_config_dsk map folder, because no need"
/bin/rm -rf /mnt/dsk2/yroot && echo "delete from node_config_dsk yroot folder, because no need"
/bin/umount /mnt/dsk0 && echo "unmount readonly robot_config_dsk"
/bin/mount -t vfat /dev/mtdblock5 /mnt/dsk0 && echo "mount read-write robot_config_dsk"
/bin/cp -rf /mnt/dsk1/home/yroot/*.yaml /mnt/dsk0/home/yroot/ && echo "copy from factory_config_dsk/home/yroot all *.yaml files to robot_config_dsk/home/yroot/"
/bin/umount /mnt/dsk0 && echo "unmount rw robot_config_dsk"
/bin/mount -t vfat -r /dev/mtdblock5 /mnt/dsk0 && echo "mount readonly robot_config_dsk"
/bin/sync && echo "/bin/sync"
/bin/sync && echo "/bin/sync"
/sbin/reboot && echo "/sbin/reboot"
